#include<iostream>
#include<cmath>
using namespace std;
int main()
{
    setlocale(LC_ALL, "Russian");
    int k;
 
    cout << "k = "; cin >> k;
 
    switch (k)
    {
        case 1: cout << "Евразия"; break;
        case 2: cout << "Африка"; break;
        case 3: cout << "Южная америка"; break;
        case 4: cout << "Северная Америка"; break;
        case 5: cout << "Австралия"; break;
        default: cout << "ошибка";
    }
 
    cout << endl << endl;
 
    system("pause");
}
